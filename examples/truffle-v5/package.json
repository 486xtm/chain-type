{
  "name": "example-truffle-v5",
  "private": true,
  "version": "0.0.5",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "typecheck": "truffle compile && pnpm generate-types && tsc --noEmit"
  },
  "dependencies": {
    "@typechain/truffle-v5": "workspace:^8.0.3",
    "typechain": "workspace:^8.2.0",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/web3": "^1.2.2",
    "web3-core": "^1",
    "web3-eth-contract": "^1",
    "web3-utils": "^1",
    "web3": "^1",
    "truffle": "^5.1.22",
    "typescript": "^4.9.5"
  }
}
